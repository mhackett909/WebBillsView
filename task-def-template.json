{
  "family": "webbills-ui",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "webbills-ui",
      "image": "${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_REGION}.amazonaws.com/webbills-ui:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "memory": 256,
      "cpu": 256,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/webbills-ui",
          "awslogs-region": "${AWS_REGION}",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "256",
  "placementConstraints": [
    {
      "type": "memberOf",
      "expression": "attribute:role == frontend"
    }
  ]
}
